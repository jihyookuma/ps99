while true do
game:GetService('StarterGui'):SetCore("DevConsoleVisible", true)

getgenv().Cynx = {
    boothSniper = {
        autoSnipe = true,
        snipeItem = {
            {Class = "item class", Name = "item name", Price = enter ur price here},
},
        sendStats = true,
        webhookURL = "", -- WEBHOOK DOESN'T WORK I NEED TO LOOK INTO IT.
    }
}

local success, result = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/ParadoxHB/Snipe/main/Sniper"))()
end)

if not success then
    warn("Error occurred while loading external script:", result)
end
task.wait(7)
end
